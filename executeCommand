  "script": "const resource = waylayUtil.getProperty(options, \"resource\") || waylayUtil.getResource(options)\nconst command = waylayUtil.getProperty(options, \"command\")\nvar properties = waylayUtil.getProperty(options, \"properties\") || {}\n\nif(_.isString(properties)){\n   properties = JSON.parse(properties)\n}\n\nwaylay.resources.executeCommand(resource, command, properties)\n.then(response =>{\n    send()\n})\n.catch(err =>{\n    send(new Error(err))\n})",
